{
  "name": "Multi-chain SDK Documentation",
  "entryPoints": [
    "packages/sdk/index.ts",
    "packages/sdk/evm/index.js",
    "packages/sdk/sui/index.ts"
  ],
  "entryPointStrategy": "packages",
  "out": "docs",
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "excludeNotDocumented": false,
  "readme": "README.md",
  "tsconfig": "tsconfig.json",
  "hideGenerator": true,
  "sort": ["source-order"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "SDK Core",
    "EVM",
    "Sui",
    "Utilities",
    "Types",
    "*"
  ],
  "kindSortOrder": [
    "Document",
    "Project", 
    "Module",
    "Namespace",
    "Enum",
    "EnumMember",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Variable",
    "Function",
    "Method",
    "Accessor"
  ],
  "plugin": ["typedoc-plugin-markdown"],
  "githubPages": false,
  "sourceLinkTemplate": "https://github.com/your-org/your-repo/blob/{gitRevision}/{path}#L{line}"
}